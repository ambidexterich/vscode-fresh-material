        {
            "name": "Red Dark",
            "scope": [
                "variable.language.this.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#be5046"
            }
        },
        {
            "name": "Red Light",
            "scope": [
                "constant.other.object.key.js",
                "entity.name.tag.close.jsx",
